    "config = {}\n",
    "def get_num_gpus(rule):\n",
    "    gres = config.get(rule, {\"gres\": \"--gres=gres:gpu:1\"}).get(\"gres\", \"--gres=gres:gpu:1\")[len(\"--gres=\") :]\n",
    "    gres_list = gres.split(':')\n",
    "    num_gpus = gres_list[-1]\n",
    "\n",
    "    return num_gpus\n",
    "\n",
    "rule= \"pbrun_fq2bam\"\n",
    "get_num_gpus(rule)"
   ]
